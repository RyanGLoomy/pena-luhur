{
    "version": 2,
    "builds": [
        {
            "src": "public/assets/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://pena-luhur.vercel.app",
        "APP_KEY": "@app_key",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "@db_host",
        "DB_PORT": "5432",
        "DB_DATABASE": "@db_database",
        "DB_USERNAME": "@db_username",
        "DB_PASSWORD": "@db_password",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr"
    }
}
